<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/keepSafe">


    <fragment
        android:id="@+id/nav_home"
        android:name="com.fmacha.final_firebase.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.fmacha.final_firebase.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.fmacha.final_firebase.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/inicio"
        android:name="com.fmacha.final_firebase.inicio"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" />
    <fragment
        android:id="@+id/keepSafe"
        android:name="com.fmacha.final_firebase.KeepSafe"
        android:label="fragment_keep_safe"
        tools:layout="@layout/fragment_keep_safe" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.fmacha.final_firebase.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>